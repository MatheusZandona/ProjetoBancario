	<queryString>
		<![CDATA[SELECT  CASE TIPO
		WHEN 0 THEN 'CC'
		WHEN 1 THEN 'CP'
		WHEN 2 THEN 'CE'
	END AS TIPO,	         
	AGENCIA, NUMERO, NOME_TITULAR, DT_ABERTURA, 
        COALESCE(SUM(VALOR), 0) AS SALDO
FROM CONTAS C
LEFT JOIN CONTAS_MOVIMENTO CM ON CM.CONTA_NUMERO = C.NUMERO
WHERE
  STATUS = 0
GROUP BY 1,2,3,4,5
ORDER BY DT_ABERTURA, C.NUMERO]]>
	</queryString>
